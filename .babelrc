{
  "presets": [
      [
          "@babel/preset-env",
          {
              "modules": false,
              "useBuiltIns": "entry",
              "corejs": 2,
          }
      ],
      "@babel/preset-react",
  ],
  "plugins": [
      ["@babel/plugin-transform-runtime",{
          "corejs":2
      }],
      ["@babel/plugin-proposal-decorators",{
          "legacy": true
      }],
      ["@babel/plugin-proposal-class-properties",{ "loose": true }],
      ["@babel/plugin-proposal-export-default-from"],
  ]
}
